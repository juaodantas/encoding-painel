    "@aws-sdk/s3-request-presigner": "^3.821.0",
    "@fastify/cors": "^11.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.2",
    "@types/bcrypt": "^5.0.2",
    "@types/passport": "^1.0.17",
    "@types/passport-local": "^1.0.38",
    "bcrypt": "^6.0.0",
    "dotenv": "^16.5.0",
    "fastify": "^5.3.3",
    "fastify-jwt": "^4.1.3",
    "fastify-passport": "^0.6.0",
    "kafkajs": "^2.2.4",
    "mysql2": "^3.14.1",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.16.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.24",
